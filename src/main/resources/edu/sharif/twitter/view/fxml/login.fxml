<AnchorPane prefHeight="650.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.sharif.twitter.view.Login">
   <children>
      <ImageView fitHeight="451.0" fitWidth="684.0" layoutX="176.0" layoutY="-50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/Twitter-complete-logo.png" />
         </image>
      </ImageView>
      <Label layoutX="302.0" layoutY="429.0" text="Password">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="300.0" layoutY="359.0" text="Username">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="468.0" layoutY="353.0">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button fx:id="loginButton" layoutX="396.0" layoutY="575.0" mnemonicParsing="false" onAction="#login" prefHeight="57.0" prefWidth="177.0" text="Login">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <PasswordField fx:id="passwordField" layoutX="468.0" layoutY="423.0">
